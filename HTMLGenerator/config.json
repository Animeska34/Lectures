{
    "path":"../",
    "directoryTemplate":"DirectoryExample.html",
    "pageTemplate":"PageExample.html",
    "sourceLink":"https://github.com/Animeska34/Lectures/tree/master/",
    "rawLink":"https://raw.githubusercontent.com/Animeska34/Lectures/master/",
    "linkTemplate" : "<li><a href = \"%link%\">%name%</a></li>",
    "unsupportedDataTemplate" : "<li><a href = \"%rawLink%\">%name%</a></li>",
    "rawDataTemplates":{
        ".cs":"<li><details><summary>%name%</summary><div class=\"code\"><pre><code class=\"language-csharp\"><textarea>%content%</textarea></code></pre></div></details></li>",
        ".xaml":"<li><details><summary>%name%</summary><div class=\"code\"><pre><code class=\"language-xml\"><textarea>%content%</textarea></code></pre></div></details></li>",
        ".js":"<li><details><summary>%name%</summary><pre><div class=\"code\"><code class=\"language-js\"><textarea>%content%</textarea></code></div></pre></details></li>",
        ".css":"<li><details><summary>%name%</summary><pre><div class=\"code\"><code class=\"language-css\"><textarea>%content%</textarea></code></div></pre></details></li>",
        ".html":"<li><details><summary>%name%</summary><a href=\"%link%\">View page</a><pre><div class=\"code\"><code class=\"language-xml\"><textarea>%content%</textarea></code></div></pre></details></li>",
        ".py":"<li><details><summary>%name%</summary><div class=\"code\"><pre><code class=\"language-python\"><textarea>%content%</textarea></code></pre></div></details></li>",
        ".png":"<li><details><summary>%name%</summary><img height = \"200\" title=\"%name%\" src=\"%rawLink%\"></img></details></li>",
        ".jpg":"<li><details><summary>%name%</summary><img height = \"200\" title=\"%name%\" src=\"%rawLink%\"></img></details></li>"
    },
    "ignoreFolders":[
        "items",
        ".vs",
        ".git",
        "HTMLGenerator",
        "css",
        "js",
        "WinUI TestApp (Package)"
    ],
    "ignoreFiles":[
        ".sln",
        ".csproj",
        ".pyproj",
        ".manifest",
        ".user"
    ],
    "output":true
}